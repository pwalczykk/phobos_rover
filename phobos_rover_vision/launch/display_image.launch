<launch>
    <node name="display_image_4" pkg="phobos_rover_vision" type="display_image" ns="rover" output="screen">
        <param name="camera_name" value="camera4"/>
        <param name="width" value="320"/>
        <param name="height" value="240"/>
        <param name="x" value="320"/>
        <param name="y" value="0"/>
        <remap from="/rover/camera4/image_raw" to="/rover/camera4/image"/>
    </node>
</launch>
